{
	"name": "DF_transaction",
	"properties": {
		"folder": {
			"name": "STG_DW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_CSV_IN1685",
						"type": "DatasetReference"
					},
					"name": "SRCaccount"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "SELECTsrc"
				},
				{
					"name": "NULL",
					"description": "null"
				}
			],
			"scriptLines": [
				"source(output(",
				"          AccountID as integer,",
				"          CustomerID as integer,",
				"          AccountNumber as string,",
				"          AccountType as string,",
				"          OpenDate as date,",
				"          CloseDate as date,",
				"          Status as string,",
				"          BranchCode as string,",
				"          Currency as string,",
				"          InterestRate as double,",
				"          CreditLimit as double,",
				"          Balance as double,",
				"          OverdraftLimit as double,",
				"          IsJointAccount as boolean,",
				"          Channel as string,",
				"          LastActivityDate as date,",
				"          {25-09-2025 14:16:3316} as timestamp,",
				"          PL_SRC_STG17 as string,",
				"          {25-09-2025 14:16:3318} as timestamp,",
				"          PL_SRC_STG19 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SRCaccount",
				"SRCaccount select(mapColumn(",
				"          AccountID,",
				"          CustomerID,",
				"          AccountNumber,",
				"          AccountType,",
				"          OpenDate,",
				"          CloseDate,",
				"          Status,",
				"          BranchCode,",
				"          Currency,",
				"          InterestRate,",
				"          CreditLimit,",
				"          Balance,",
				"          OverdraftLimit,",
				"          IsJointAccount,",
				"          Channel,",
				"          LastActivityDate,",
				"          created_date = {25-09-2025 14:16:3316},",
				"          created_by = PL_SRC_STG17,",
				"          updated_date = {25-09-2025 14:16:3318},",
				"          updated_by = PL_SRC_STG19",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SELECTsrc",
				"SELECTsrc derive(each(match(type=='string'), $$ = iifNull($$, 'UNK')),",
				"          each(match(type=='integer'), $$ = iifNull($#, 0)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$, toTimestamp('1900-01-01 00:00:00', 'dd-MM-yyyy 00:00:00')))) ~> NULL"
			]
		}
	}
}