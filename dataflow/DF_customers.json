{
	"name": "DF_customers",
	"properties": {
		"folder": {
			"name": "src_to_stg"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_CSV_IN1680",
						"type": "DatasetReference"
					},
					"name": "SRCcustomers"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_CSV_IN1680",
						"type": "DatasetReference"
					},
					"name": "SNKcustomers"
				}
			],
			"transformations": [
				{
					"name": "derivedaudit"
				}
			],
			"scriptLines": [
				"parameters{",
				"     filename as string",
				"}",
				"source(output(",
				"          CustomerID as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Gender as string,",
				"          DateOfBirth as date,",
				"          Email as string,",
				"          Phone as string,",
				"          AddressLine1 as string,",
				"          AddressLine2 as string,",
				"          City as string,",
				"          State as string,",
				"          Country as string,",
				"          PostalCode as integer,",
				"          MaritalStatus as string,",
				"          Occupation as string,",
				"          AnnualIncome as double,",
				"          RiskCategory as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SRCcustomers",
				"SRCcustomers derive(created_at = currentTimestamp(),",
				"          updated_at = currentTimestamp(),",
				"          created_by = 'CP_src_transaction_to_sql_transaction') ~> derivedaudit",
				"derivedaudit sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:[($filename)],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> SNKcustomers"
			]
		}
	}
}