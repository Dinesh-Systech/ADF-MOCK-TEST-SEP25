{
	"name": "DF_Trans_Incr",
	"properties": {
		"folder": {
			"name": "SRC_STG"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_CSV_IN1685",
						"type": "DatasetReference"
					},
					"name": "SRCtransaction"
				},
				{
					"dataset": {
						"referenceName": "DS_CSV_IN1685",
						"type": "DatasetReference"
					},
					"name": "TGTtransactions"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "LEFTsrctgt"
				},
				{
					"name": "SELECTtgt"
				},
				{
					"name": "split1"
				},
				{
					"name": "AUDITcols"
				},
				{
					"name": "AGGRmaxdate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_col0_} as string,",
				"          {_col1_} as string,",
				"          {_col2_} as string,",
				"          {_col3_} as string,",
				"          {_col4_} as string,",
				"          {_col5_} as string,",
				"          {_col6_} as string,",
				"          {_col7_} as string,",
				"          {_col8_} as string,",
				"          {_col9_} as string,",
				"          {_col10_} as string,",
				"          {_col11_} as string,",
				"          {_col12_} as string,",
				"          {_col13_} as string,",
				"          {_col14_} as string,",
				"          {_col15_} as string,",
				"          {_col16_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SRCtransaction",
				"source(output(",
				"          {_col0_} as string,",
				"          {_col1_} as string,",
				"          {_col2_} as string,",
				"          {_col3_} as string,",
				"          {_col4_} as string,",
				"          {_col5_} as string,",
				"          {_col6_} as string,",
				"          {_col7_} as string,",
				"          {_col8_} as string,",
				"          {_col9_} as string,",
				"          {_col10_} as string,",
				"          {_col11_} as string,",
				"          {_col12_} as string,",
				"          {_col13_} as string,",
				"          {_col14_} as string,",
				"          {_col15_} as string,",
				"          {_col16_} as string,",
				"          {_col17_} as string,",
				"          {_col18_} as string,",
				"          {_col19_} as string,",
				"          {_col20_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> TGTtransactions",
				"SRCtransaction, SELECTtgt join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> LEFTsrctgt",
				"AGGRmaxdate select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SELECTtgt",
				"LEFTsrctgt split(ERROR_FUNCTION(''),",
				"     ERROR_FUNCTION(''),",
				"     disjoint: true) ~> split1@(INSERTstream, UPDATEstream)",
				"split1@INSERTstream derive() ~> AUDITcols",
				"TGTtransactions aggregate() ~> AGGRmaxdate"
			]
		}
	}
}